{
  "name": "live-server",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx --env-file=.env.development --watch src",
    "start": "node --env-file=.env.production dist/src/index.js",
    "production": "run-p cloudflared start",
    "cloudflared": "cloudflared tunnel --config ./cloudflared/config.yaml run quina-api"
  },
  "dependencies": {
    "@trpc/server": "11.7.2",
    "cors": "2.8.5",
    "date-fns": "4.1.0",
    "lowdb": "7.0.1",
    "seedrandom": "3.0.5",
    "superjson": "2.2.6",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/node": "24.10.1",
    "@types/seedrandom": "3.0.8",
    "npm-run-all": "4.1.5",
    "start-server-and-test": "2.1.3",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "wait-port": "1.1.0"
  }
}
